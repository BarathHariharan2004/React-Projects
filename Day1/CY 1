import React,{useState} from 'react';
import './App.css';
const app2=()=>{
  const name1="Barath Hariharan";
  const name2="Barath Hariharan";
  const obj1={name:"Barath Hariharan"}
  const obj2={name:"Barath Hariharan"}
  if(name1===name2)
  {
    alert(true);
  }else{
    alert(false);
  }
  if(obj1.name === obj2.name)
  {
    alert(true);
  }else{
    alert(false);
  }
}
 const App=()=>{
  return(
    <div>
      <button onClick={app2}>Click</button>
      {/*<p>This is World Functional Component</p>*/}
    </div>
  )
 }


